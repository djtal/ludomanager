if(@context == :account_game)
  page[dom_id(@account_game)].fade();
elsif (@context == :game)
  page[dom_id(@account_game.game)].down(".account_managment").replace(render(:partial => 'games/account_managment', :locals => {:game => @account_game.game}))
  page[dom_id(@account_game.game)].down(".account_managment").highlight
end