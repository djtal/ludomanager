Publique;Nom;Duree;Annee de sortie;Editeur;Joueur
<% @account_games.each do |target, games| -%>
  <%= "#{Game::Target[target].first} - #{games.size}" %>
  <% games.each do |g| -%>
    ;<%= [g.name, Game::TimeCategory[g.time_category].first, g.publish_year, g.editor] * ";" %>;<%= (g.min_player..g.max_player).to_a * ";" %>
  <% end %>
<% end %>